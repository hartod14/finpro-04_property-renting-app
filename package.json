{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "serve": "turbo run serve",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install",
    "postprepare": "bash -c 'chmod ug+x .husky/*'"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.3",
    "@types/express": "^5.0.1",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "husky": "^8.0.3",
    "lint-staged": "^15.5.1",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tsc-alias": "^1.8.15",
    "tsconfig": "^7.0.0",
    "turbo": "latest"
  },
  "name": "finpro-nextjs-express-prisma",
  "packageManager": "npm@8.15.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged"
    }
  },
  "dependencies": {
    "@types/axios": "^0.14.4",
    "axios": "^1.8.4"
  }
}
